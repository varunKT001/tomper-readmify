<p align='center'>
<h1 align='center'>👋 Hi, I am <%= typeof fullName === 'string' && fullName %></h2>
<p align='center'><% if (typeof fieldsOfWork === 'object') { for(let field of fieldsOfWork) { %><%= typeof field === 'string' && (field.charAt(0).toUpperCase() + field.slice(1)) %> <%= fieldsOfWork.indexOf(field) === fieldsOfWork.length - 1 ? '' : ' | ' %><% } } %></h4>
</p>

<% if (typeof aboutMe === 'object' && aboutMe.length > 0) { %>
## About Me 👦
<% for (let item of aboutMe) { %> 
- <%= `${item.emoji} ${item.title.trim()} ${item.link !== '' ? `[${item.name}](${item.link})` : `${item.name}.`}` -%>
<% } %>
<% } %> 

<% if (typeof achievements === 'object' && achievements.length > 0) { %>
## Some achievements 🏆
<% for (let item of achievements) { %> 
- <%= `${item.link !== '' ? `[${item.title}](${item.link})` : `${item.title}.`}` -%>
<% } %>
<% } %>

<% if (typeof skills === 'object' && skills.length > 0) { %>
## Skills 💪
<p>
<% for (let skill of skills) { -%>
<!-- <%= skill.skill %>  -->
<img width='80px' height='25px' style="width:80px;height:25px" src='<%= skill.url %>' />
<% } %>
</p>
<% } %>

<% if (typeof stats === 'object' && stats.show === true) { %>
---

<h2 align="center">📊 My Stats</h2>

<% if (stats.github.streaks.show) { %>
<p align="center"><img align="center" src="https://github-readme-streak-stats-2.herokuapp.com/?user=<%= githubUsername %>&theme=<%= stats.github.streaks.theme%> " alt="<%= githubUsername %>" /></p>
<% } %> 

<% if (stats.github.contributions.show) { %>
<p align="center"><img align="center" src="https://github-readme-stats.vercel.app/api?username=<%= githubUsername %>&theme=<%= stats.github.contributions.theme %>&show_icons=true&locale=en" alt="<%= githubUsername %>" /></p>
<% } %>
<% } %>

<% if (typeof socials === 'object' && socials.length > 0) { %>
<h3 align="center">Connect with me:</h3>
<p align="center">
<% for (let social of socials) { -%>
<a href="<%= social.url %>" target="blank"><img align="center" src="<%= social.icon %>" alt="<%= githubUsername %>" height="30px" width="40px" /></a>
<% } %>
</p>
<% } %>

<% if (typeof stats.profileViews === 'object' && stats.profileViews.show === true) { %>
<p align="center"><img src="https://komarev.com/ghpvc/?username=<%= githubUsername %>&color=<%= stats.profileViews.theme %>&style=<%= stats.profileViews.style %>&label=<%= stats.profileViews.label %>" alt="<%= githubUsername %> " /></p>
<% } %>

<% if (typeof githubUsername === 'string') { %>
<br/>
<br/>

---

<p align='center'><%= `Made with 💙 by ${githubUsername}` %></p>
<% } %>
